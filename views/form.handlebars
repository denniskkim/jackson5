            <!-- PAGE CONTENT WRAPPER -->
            <div class="page-content-wrap">
                <!-- Start Form Builder-->
                <div class="row clearfix">
                    <!--<iframe id="formbuilder_iframe" src="http://bootsnipp.com/j87klPolka/formbuilder3.html" style="width:100%; height:1000px;" frameborder="0"></iframe>-->
                    <div class="col-md-12">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">Edit Forms</h3>
                            </div>
                            <div class="panel-body">
                                <!-- Components -->
                                <div class="col-md-6">
                                    <p>Drag and drop components from the left side of the right side to create your patient form! </p>
                                    <div class="tabbable" style="padding-top: 50px;">
                                        <ul class="nav nav-tabs" id="formtabs">
                                            <!-- Tab nav -->
                                        </ul>
                                        <hr>
                                        <form class="form-horizontal" id="components">
                                            <fieldset>
                                                <div class="tab-content">
                                                    <!-- Tabs of snippets go here -->
                                                </div>
                                            </fieldset>
                                        </form>
                                    </div>
                                </div>
                                <!-- / Components -->
                                <!-- Building Form. -->
                                <div class="col-md-6">
                                    <div class="clearfix">
                                        <h2 style="padding-top: 75px;">Your Form</h2>
                                        <hr>
                                        <div id="build">
                                            <form id="target" class="form-horizontal">
                                            </form>
                                        </div>
                                    </div>
                                    <form class="form-horizontal" method="POST">
                                        <div class="form-group">
                                            <input type="hidden" id="hiddenformhtml" name="form">
                                        </div>
                                        <div>
                                            <div class="col-lg-12 col-lg-offset-5">
                                                <button id="saveform" class="btn btn-success" onclick="test()">Save Form</button>
                                            </div>
                                        </div>

                                    </form>
                                </div>
                                <!-- / Building Form. -->
                            </div>
                        </div>
                    </div>
                </div>
                <!-- End form builder-->
                <script data-main="http://bootsnipp.com/j87klPolka/assets3/js/main-built.js" src="http://bootsnipp.com/j87klPolka/assets3/js/lib/require.js" ></script>

            </div>

    <!-- Start This Page Script-->
    <script>
        test = function(){
            document.getElementById("hiddenformhtml").value = document.getElementById("render").value;
        };
