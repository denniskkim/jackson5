                            <div class="widget-title">Most recent Checkin</div>
                            <div class="widget-subtitle">On your website</div>
                            <div class="widget-subtitle">{{{patients.0.checkinTime}}}</div>

                        </div>
                    </div>
                    <!-- END WIDGET REGISTRED -->

                </div>
                <div class="col-md-3">

                    <!-- START WIDGET CLOCK -->
